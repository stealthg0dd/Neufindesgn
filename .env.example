# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
VITE_SUPABASE_PROJECT_ID=your-project-id

# Backend API Configuration
VITE_BACKEND_URL=https://neufindesgn-production.up.railway.app
VITE_FRONTEND_URL=https://neufindesgn.vercel.app

# Feature Flags
VITE_ENABLE_DEMO=false
VITE_LOG_LEVEL=error

# Market Data / External APIs
VITE_FINNHUB_API_KEY=your-finnhub-api-key
VITE_ALPHA_VANTAGE_KEY=your-alpha-vantage-key
VITE_NEWSAPI_KEY=your-newsapi-key
VITE_OPENAI_KEY=your-openai-api-key
